@font-face {
  font-family: $font-regular;
  src: local('SFProText Regular'),
  local('SFProText-Regular'),
  local('SF Pro Text Regular'),
  font-url('SFProText-Regular.woff2'),
  font-url('SFProText-Regular.eot?#iefix') format('embedded-opentype'),
  font-url('SFProText-Regular.woff') format('woff'),
  font-url('SFProText-Regular.ttf') format('truetype'),
  font-url('SFProText-Regular.svg#SFProText-Regular') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: auto;
}

